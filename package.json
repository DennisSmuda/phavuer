{
  "name": "phavuer",
  "version": "0.17.0",
  "description": "A wrapper library that integrates Phaser 3 with Vue 3.",
  "main": "dist/phavuer.umd.js",
  "module": "dist/phavuer.es.js",
  "types": "types/phavuer.d.ts",
  "exports": {
    ".": {
      "import": "./dist/phavuer.es.js",
      "require": "./dist/phavuer.umd.js",
      "types": "./types/phavuer.d.ts"
    }
  },
  "author": "Laineus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/laineus/phavuer.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "preview": "vite preview",
    "test": "vitest"
  },
  "files": [
    "dist",
    "src",
    "types"
  ],
  "peerDependencies": {
    "phaser": "^3.70.0",
    "vue": "^3.3.13"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/browser": "^1.3.0",
    "@vue/compiler-sfc": "^3.3.13",
    "@vue/test-utils": "^2.4.4",
    "mock-require": "^3.0.3",
    "phaser": "^3.70.0",
    "vite": "^5.0.0",
    "vitest": "^1.3.0",
    "vue": "^3.3.13",
    "webdriverio": "^8.32.2"
  }
}
